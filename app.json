{
  "expo": {
    "name": "WalletApp",
    "slug": "WalletApp",
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./src/assets/ios/Icon-dark-pastel.png",
    "splash": {
      "image": "./src/assets/ios/iPhone12-12Pro-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kazuma0129.work.WalletApp",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app will optionally use Face ID or Touch ID to save login"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/ios/icon-dark-pastel.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./src/assets/ios/icon-dark-pastel.png"
    }
  }
}
